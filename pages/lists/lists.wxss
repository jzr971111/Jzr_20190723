
page{
  background-color: lightcyan;
}

.warp{
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
}

navigator { overflow: hidden;}

.list{
  margin-bottom: 20px;
  height: 100px;
  position: relative;
}

.imgs {
  float: left;
}

.imgs image {
  display: block; 
  width: 200rpx; 
  height: 200rpx; 
}

.infos{
  float: left; 
  width: 400rpx; 
  height: 200rpx; 
  padding: 20rpx 0 0 20rpx;}
.title{
  font-size: 20px;
}
.date {
  float: right; 
  font-size: 16px; 
  color: #999; 
  position: absolute;
  bottom: 0;
}
